{
  "name": "minicomps",
  "version": "0.1.0",
  "description": "minimal ui components",
  "main": "rollup.config.js",
  "scripts": {
    "uglify": "uglifyjs dist/minicomps.js -o dist/minicomps.min.js",
    "uglify-mod": "uglifyjs dist/minicomps.mjs -o dist/minicomps.min.mjs",
    "build": "rollup -c && npm run uglify && npm run uglify-mod",
    "docs": "jsdoc src/* ./README.md -t ./node_modules/clean-jsdoc-theme"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bit101/minicomps.git"
  },
  "author": "Keith Peters",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bit101/minicomps/issues"
  },
  "homepage": "https://github.com/bit101/minicomps#readme",
  "devDependencies": {
    "clean-jsdoc-theme": "^4.3.0",
    "jsdoc": "^4.0.4",
    "rollup": "^2.48.0",
    "uglify-js": "^3.13.7"
  }
}
